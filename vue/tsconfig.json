{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/versions.ts",
    "src/**/*.ts",
    "decs.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "vite-plugin-pwa/client"
    ],
    "importsNotUsedAsValues": "remove",
    "isolatedModules": false,
    "useDefineForClassFields": false,
    "lib": [
      "es2021"
    ]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
