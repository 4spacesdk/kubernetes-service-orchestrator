{
  "name": "4spacesdk/kubernetes-service-orchestrator",
  "description": "Service Orchestrator for Kubernetes",
  "require": {
    "php": ">=8.3",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-intl": "*",
    "ext-yaml": "*",
    "ext-zmq": "*",
    "ext-sqlsrv": "*",
    "ext-openssl": "*",
    "hellogerard/jobby": "v3.5.0",
    "codeigniter4/framework": "v4.4.5",
    "4spacesdk/ci4debugtool": "1.0.8",
    "4spacesdk/ci4ormextension": "v1.0.9",
    "4spacesdk/ci4restextension": "v1.0.6",
    "4spacesdk/ci4authextension": "v1.0.1",
    "renoki-co/php-k8s": "3.8.1",
    "google/cloud-artifact-registry": "^0.3.7",
    "podio-community/podio-php": "7.0.2",
    "knplabs/github-api": "^v3.9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1"
  },
  "scripts": {
    "pre-autoload-dump": [
      "rm -rf vendor/podio-community/podio-php/lib/vendor/kint"
    ]
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
